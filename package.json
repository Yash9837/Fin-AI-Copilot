{
  "name": "fin-ai-copilot",
  "version": "1.0.0",
  "private": true,
  "description": "AI-powered customer support platform with free Hugging Face integration",
  "author": "Your Name",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "prebuild": "npm run clean",
    "postbuild": "echo 'âœ… Build completed successfully!'",
    "typecheck": "echo 'No TypeScript check needed (JavaScript project)'",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,json,css,md}\"",
    "prod:build": "NODE_ENV=production npm run build",
    "prod:start": "NODE_ENV=production npm start",
    "docker:build": "docker build -t fin-ai-copilot .",
    "docker:run": "docker run -p 3000:3000 fin-ai-copilot",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health:check": "curl http://localhost:3000/api/health",
    "verify": "npm run lint && npm run format:check && npm run build"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "nextjs",
    "react",
    "ai",
    "chatbot",
    "customer-support",
    "huggingface",
    "llama",
    "free-ai",
    "tailwindcss"
  ]
}
